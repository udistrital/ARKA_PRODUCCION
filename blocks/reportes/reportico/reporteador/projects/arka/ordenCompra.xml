<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Orden de Compra</ReportTitle><ReportDescription>Seleccione los criterios de b√∫squeda</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>no</gridSearchable><gridPageable>no</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT DISTINCT consecutivo_compras, 
       orden.fecha_registro, id_elemento_ac,
       &quot;RUB_NOMBRE_RUBRO&quot; rubro_nombre,
       &quot;ESF_SEDE&quot; sede, 
       &quot;ESF_DEP_ENCARGADA&quot; dependencia_solicitante,
       nombre_razon_social , 
       &quot;FUN_NOMBRE&quot; nombre_supervisor, 
        &quot;ORG_NOMBRE&quot; nombre_ordenador,
       elemento_nombre nombre_nivel,tipo_bienes.descripcion tipo_bien,marca, serie,
       ear.descripcion, 
       cantidad, unidad, valor,  subtotal_sin_iva, total_iva, total_iva_con       
  FROM orden
 left  JOIN supervisor_servicios ss ON ss.id_supervisor=orden.id_supervisor
  left JOIN contratista_servicios cs ON cs.id_contratista=orden.id_contratista
  left JOIN arka_parametros.arka_funcionarios fn ON ss.nombre=cast(&quot;FUN_IDENTIFICACION&quot; as character varying)
  JOIN (SELECT DISTINCT &quot;ESF_CODIGO_DEP&quot;, &quot;ESF_DEP_ENCARGADA&quot;
FROM arka_parametros.arka_dependencia) as ad ON ad.&quot;ESF_CODIGO_DEP&quot;=orden.dependencia_solicitante
  JOIN arka_parametros.arka_sedes sa ON sa.&quot;ESF_ID_SEDE&quot;=orden.sede
  JOIN arka_parametros.arka_ordenadores ON &quot;ORG_IDENTIFICACION&quot;=id_ordenador_encargado
 JOIN elemento_acta_recibido ear ON ear.id_orden=orden.id_orden
  JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=nivel
  JOIN tipo_bienes ON tipo_bienes.id_tipo_bienes=tipo_bien
  JOIN arka_parametros.arka_rubros ar ON rubro=ar.&quot;RUB_IDENTIFICADOR&quot;
  WHERE 1=1
  AND tipo_orden=1
  AND orden.estado=TRUE
  AND &quot;ORG_ESTADO&quot;='A'
ORDER BY consecutivo_compras</SQLRaw></SQL><QueryColumns><QueryColumn><Name>consecutivo_compras</Name><TableName></TableName><ColumnName>DISTINCT</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>fecha_registro</Name><TableName>orden</TableName><ColumnName>fecha_registro</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>rubro_nombre</Name><TableName></TableName><ColumnName>&quot;RUB_NOMBRE_RUBRO&quot;</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>nombre_razon_social</Name><TableName></Tab